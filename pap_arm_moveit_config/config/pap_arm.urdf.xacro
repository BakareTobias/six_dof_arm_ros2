<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="pap_arm">
    <xacro:arg name="initial_positions_file" default="initial_positions.yaml" />

    <!-- Import pap_arm urdf file -->
    <xacro:include filename="$(find pap_arm)/urdf/pap_arm.xacro" />

    <!-- Import control_xacro -->
    <xacro:include filename="pap_arm.ros2_control.xacro" />


    <xacro:pap_arm_ros2_control name="FakeSystem" initial_positions_file="$(arg initial_positions_file)"/>

</robot>
